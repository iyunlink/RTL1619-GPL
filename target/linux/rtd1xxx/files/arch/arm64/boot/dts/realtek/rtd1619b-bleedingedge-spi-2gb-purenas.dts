};

&audio_heap {
	status = "disabled";
};

&media_heap {
	status = "disabled";
};

&cma_resrved_1 {
	status = "disabled";
};

&cma_resrved_2 {
	status = "disabled";
};

&cma_resrved_3 {
	status = "disabled";
};

&cma_resrved_4 {
	status = "disabled";
};

&cma_resrved_5 {
