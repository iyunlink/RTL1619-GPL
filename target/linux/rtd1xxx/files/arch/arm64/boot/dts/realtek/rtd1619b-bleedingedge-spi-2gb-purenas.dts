&reserved_memory {
       ramoops@22000000  {
               compatible = "ramoops";
               reg = <0x22000000 0x00200000>;
               console-size = <0x00100000>;
               record-size = <0x00004000>;
               ftrace-size = <0x00004000>;
       };
};
